msgid "uncodable character"
msgstr "nekódovatelný znak"

#: src/Paragraph.cpp:2472
msgid "Memory problem"
msgstr "Interní chyba"

#: src/Paragraph.cpp:2472
msgid "Paragraph not properly initiliazed"
msgstr "Odstavec není správně inicializován"

