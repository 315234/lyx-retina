<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>QNoteDialogBase</class>
<widget class="QDialog">
    <property name="name">
        <cstring>QNoteDialogBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>188</width>
            <height>192</height>
        </rect>
    </property>
    <property name="caption">
        <string>QNoteDialogBase</string>
    </property>
    <property name="sizeGripEnabled">
        <bool>true</bool>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QPushButton" row="1" column="0">
            <property name="name">
                <cstring>okPB</cstring>
            </property>
            <property name="text">
                <string>&amp;OK</string>
            </property>
            <property name="default">
                <bool>false</bool>
            </property>
        </widget>
        <widget class="QPushButton" row="1" column="1">
            <property name="name">
                <cstring>closePB</cstring>
            </property>
            <property name="text">
                <string>Close</string>
            </property>
            <property name="default">
                <bool>true</bool>
            </property>
        </widget>
        <widget class="QButtonGroup" row="0" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>NoteBG</cstring>
            </property>
            <property name="title">
                <string>Type</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>noteRB</cstring>
                    </property>
                    <property name="text">
                        <string>LyX &amp;Note</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>LyX internal only</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>commentRB</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Comment</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Export to LaTeX/Docbook but don't print</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>greyedoutRB</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Greyed out</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Print as grey text</string>
                    </property>
                </widget>
            </vbox>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>noteRB</sender>
        <signal>toggled(bool)</signal>
        <receiver>QNoteDialogBase</receiver>
        <slot>change_adaptor()</slot>
    </connection>
    <connection>
        <sender>greyedoutRB</sender>
        <signal>toggled(bool)</signal>
        <receiver>QNoteDialogBase</receiver>
        <slot>change_adaptor()</slot>
    </connection>
    <connection>
        <sender>commentRB</sender>
        <signal>toggled(bool)</signal>
        <receiver>QNoteDialogBase</receiver>
        <slot>change_adaptor()</slot>
    </connection>
</connections>
<includes>
    <include location="global" impldecl="in declaration">config.h</include>
    <include location="local" impldecl="in declaration">qt_helpers.h</include>
</includes>
<slots>
    <slot>change_adaptor()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
