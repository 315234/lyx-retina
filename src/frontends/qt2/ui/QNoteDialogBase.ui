<!DOCTYPE UI><UI>
<class>QNoteDialogBase</class>
<include location="global">config.h</include>
<include location="local">qt_helpers.h</include>
<widget>
    <class>QDialog</class>
    <property stdset="1">
        <name>name</name>
        <cstring>QNoteDialogBase</cstring>
    </property>
    <property stdset="1">
