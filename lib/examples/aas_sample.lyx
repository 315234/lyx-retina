\begin_inset Formula \[
d_{1}=\sqrt{\left( \begin{array}{c}
\frac{x_{1}}{R_{maj}}
\end{array}\right) ^{2}+\left( \begin{array}{c}
\frac{y_{1}}{R_{min}}
\end{array}\right) ^{2}}\]

\end_inset


\begin_inset Formula \[
d_{2}=\sqrt{\left( \begin{array}{c}
\frac{x_{1}}{PR_{maj}}
\end{array}\right) ^{2}+\left( \begin{array}{c}
\case {y_{1}}{PR_{min}}
